<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="360.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ListView fx:id="editMaterialsList" layoutX="14.0" layoutY="44.0" prefHeight="170.0" prefWidth="200.0" />
        <ListView fx:id="editMaterialsList2" layoutX="426.0" layoutY="14.0" onMouseClicked="#handleEditMaterialsWindow" prefHeight="200.0" prefWidth="200.0" />
        <Label layoutX="306.0" layoutY="105.0" text="&gt;&gt;" />
        <Button fx:id="addMaterialToListButton" layoutX="64.0" layoutY="230.0" mnemonicParsing="false" onAction="#handleEditMaterialsWindow" prefHeight="26.0" prefWidth="100.0" text="Dodaj" />
        <Button fx:id="materialWindowUpdateButton" layoutX="476.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleEditMaterialsWindow" text="Zaktualizuj ilość" />
        <Label layoutX="426.0" layoutY="235.0" text="Ilość:" />
        <TextField fx:id="articleMaterialsQuantity" layoutX="452.0" layoutY="231.0" />
        <Button fx:id="deleteFromMaterials" layoutX="476.0" layoutY="292.0" mnemonicParsing="false" onAction="#handleEditMaterialsWindow" prefHeight="26.0" prefWidth="100.0" text="Usuń" />
