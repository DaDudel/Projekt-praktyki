      <Button fx:id="plusButton" layoutX="1192.0" layoutY="407.0" mnemonicParsing="false" onAction="#plusQuantity" prefWidth="25.6" text="+" />
      <Button fx:id="minusButton" layoutX="1218.0" layoutY="407.0" mnemonicParsing="false" onAction="#minusQuantity" prefWidth="25.6" text="-" />
